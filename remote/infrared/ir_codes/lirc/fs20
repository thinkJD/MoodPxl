 // AV		->		0x00000, // Menu	->		0x00000, // PiP		->		0x00000, // A/B		->		0x00000, // 16:9	->		0x00000, // Info	->		0x00000, // VTX1	->		0x00000, // VTX2	->		0x00000, // VTX3	->		0x00000, // Blue	->		0x00000, // Yello	->		0x00000, // Green	->		0x00000, // Red		->		0x00000, // TV		->		0x00000  // Power	->	Power},